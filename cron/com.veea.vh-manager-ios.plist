<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.veea.vh-manager-ios</string>
		<key>ProgramArguments</key>
		<array>
			<string>sh</string>
			<string>-c</string>
			<string>/Users/deploy/cronwork/vh-manager-ios/cron/run.sh</string>
		</array>
		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>7</integer>
			<key>Minute</key>
			<integer>00</integer>
		</dict>
		<key>UserName</key>
		<string>deploy</string>
		<key>WorkingDirectory</key>
		<string>/Users/deploy/cronwork/vh-manager-ios/</string>
		<key>StandardErrorPath</key>
		<string>/tmp/vh-manager-ios.err</string>
		<key>StandardOutPath</key>
		<string>/tmp/vh-manager-ios.out</string>
	</dict>
</plist>

